{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "startServerCommand": "npm run build && npm run preview",
      "startServerReadyPattern": "ready",
      "url": [
        "http://localhost:4321",
        "http://localhost:4321/services",
        "http://localhost:4321/pricing",
        "http://localhost:4321/about",
        "http://localhost:4321/contact",
        "http://localhost:4321/resources",
        "http://localhost:4321/locations",
        "http://localhost:4321/locations/royse-city",
        "http://localhost:4321/locations/rockwall",
        "http://localhost:4321/locations/wylie",
        "http://localhost:4321/locations/dallas"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": 0.9}],
        "categories:accessibility": ["error", {"minScore": 0.9}],
        "categories:best-practices": ["error", {"minScore": 0.95}],
        "categories:seo": ["error", {"minScore": 0.95}],
        "categories:pwa": "off"
      },
      "budgets": [
        {
          "path": "/",
          "resourceSizes": [
            {
              "resourceType": "total",
              "budget": 1200
            },
            {
              "resourceType": "script",
              "budget": 250
            },
            {
              "resourceType": "image",
              "budget": 1200
            }
          ],
          "resourceCounts": [
            {
              "resourceType": "third-party",
              "budget": 10
            }
          ],
          "timings": [
            {
              "metric": "largest-contentful-paint",
              "budget": 2300
            },
            {
              "metric": "total-blocking-time",
              "budget": 100
            },
            {
              "metric": "cumulative-layout-shift",
              "budget": 0.1
            }
          ]
        }
      ]
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}