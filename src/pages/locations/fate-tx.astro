---
import CityService from '../../components/CityService.astro';

const city = "Fate";
const county = "Rockwall";
const population = "12,000+ residents";
const industries = [
  "Construction & Real Estate",
  "Retail & Hospitality",
  "Professional Services",
  "Healthcare"
];

const testimonial = {
  name: "Sarah Martinez",
  business: "Martinez Construction",
  quote: "Modern Ledger helped us scale our construction business during Fate's rapid growth. Their expertise in job costing and subcontractor management saved us thousands."
};

const content = {
  intro: "Fate, TX is experiencing rapid growth with new residential and commercial developments. Modern Ledger provides comprehensive bookkeeping services to local businesses, helping them maintain accurate financial records during this expansion phase.",
  benefits: [
    "QuickBooks setup and training",
    "Monthly bookkeeping and reconciliation",
    "Job costing for construction businesses",
    "Tax preparation support",
    "Financial reporting and analysis"
  ],
  cta: "Ready to Scale Your Fate Business?"
};
---

<CityService
  city={city}
  county={county}
  population={population}
  industries={industries}
  testimonial={testimonial}
  content={content}
/>