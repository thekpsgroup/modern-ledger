---
import CityService from '../../components/CityService.astro';

const city = "Rockwall";
const county = "Rockwall";
const population = "45,000";
const industries = ["retail", "healthcare", "professional services", "e-commerce"];

const testimonial = {
  name: "Jennifer Martinez",
  business: "Rockwall Family Dentistry",
  quote: "Modern Ledger transformed our practice finances. We went from dreading month-end to having clear, accurate reports that actually help us make decisions. Their local knowledge of Texas healthcare regulations is invaluable."
};

const content = {
  intro: `Rockwall, Texas is experiencing rapid growth as families and businesses discover this charming suburb just 30 minutes from Dallas. With a population of ${population} and counting, Rockwall is home to thriving retail districts, healthcare practices, professional services firms, and e-commerce businesses. As a Rockwall-based bookkeeping service, Modern Ledger understands the unique financial challenges of businesses in our community.`,
  benefits: [
    "Local expertise in Texas business regulations and tax requirements",
    "Same-day response times for Rockwall businesses",
    "Understanding of local economic conditions and market trends",
    "Personal relationships with Rockwall financial institutions"
  ],
  cta: "Talk to a Rockwall-Based Bookkeeping Expert"
};
---

<CityService
  city={city}
  county={county}
  population={population}
  industries={industries}
  testimonial={testimonial}
  content={content}
/>