---
import CityService from '../../components/CityService.astro';

const city = "Rowlett";
const county = "Dallas";
const population = "65,000+ residents";
const industries = [
  "Retail & E-commerce",
  "Professional Services",
  "Healthcare & Dental",
  "Construction & Home Services",
  "Food Service & Hospitality"
];

const testimonial = {
  name: "Mike Rodriguez",
  business: "Rowlett Family Dental",
  quote: "Modern Ledger transformed our practice's financial management. Their healthcare bookkeeping expertise and QuickBooks automation saved us 15 hours per month."
};

const content = {
  intro: "Rowlett, TX is a thriving suburb of Dallas known for its excellent schools and family-friendly environment. Modern Ledger serves local businesses with comprehensive bookkeeping services, from retail shops to professional practices.",
  benefits: [
    "Retail inventory accounting",
    "Healthcare practice bookkeeping",
    "Payroll processing and compliance",
    "Tax preparation and planning",
    "Financial reporting and analysis"
  ],
  cta: "Ready to Grow Your Rowlett Business?"
};
---

<CityService
  city={city}
  county={county}
  population={population}
  industries={industries}
  testimonial={testimonial}
  content={content}
/>